/* component/Topbar/Topbar.wxss */
.wx-topbar-container .wx-topbar{
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  text-align: center;
  box-sizing: content-box!important;
  background: #fff;
}
.wx-topbar-container .wx-topbar .wx-topbar-title{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  font-weight: bold;
  color: #17233D;
  height: 100%;
}
.wx-topbar-container .wx-topbar>.icon{
  position: absolute;
  left: 16rpx;
  top: 50%;
  transform: translateY(-50%);
}
